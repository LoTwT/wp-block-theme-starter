{
  "name": "wp-block-theme-starter",
  "version": "0.0.0",
  "packageManager": "pnpm@7.12.1",
  "description": "wp-block-theme-starter",
  "scripts": {
    "start": "wp-scripts start",
    "build": "wp-scripts build",
    "lint": "prettier -c .",
    "prettier": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks",
    "up": "taze latest -I"
